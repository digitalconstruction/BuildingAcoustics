<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#"
     xml:base="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/">
    <owl:Ontology rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics">
        <owl:imports rdf:resource="http://www.w3.org/ns/ssn/"/>
        <owl:imports rdf:resource="https://w3id.org/bot#"/>
        <owl:imports rdf:resource="https://w3id.org/digitalconstruction/0.3/Information"/>
        <dc:creator>RISE (ri.se)</dc:creator>
        <dc:creator>SUITE5 (suite5.eu)</dc:creator>
        <terms:abstract>Extension of W3C Semantic Sensor Network Ontology (SSN) with representation of building acoustics.</terms:abstract>
        <terms:contributor>Seppo Törmä</terms:contributor>
        <terms:description>The IAQ Ontology  formalise the model to be considered for IAQ modeling in the building environment.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:title>Digital Construction - BuildingAcoustics</terms:title>
        <vann:preferredNamespacePrefix>dicba</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/digitalconstruction/0.3/BuildingAcoustics</vann:preferredNamespaceUri>
        <rdfs:comment>The IAQ ontology was created by SUITE5 in the BIM4EEB project (GA 820660), supported through the EU Horizon 2020 Research and Innovation Programme under the topic Building information modelling adapted to efficient renovation (RIA). </rdfs:comment>
        <rdfs:label>Digital Construction - BuildingAcoustics</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://w3id.org/digitalconstruction"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/terms/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/abstract"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AverageNoiseLevel -->


    <rdfs:Datatype rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AverageNoiseLevel"/>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Noise -->


    <rdfs:Datatype rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Noise"/>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#PercentilNoiseLevel -->


    <rdfs:Datatype rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#PercentilNoiseLevel"/>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#ReverberationTime -->


    <rdfs:Datatype rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#ReverberationTime"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/bot#hasElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasElement"/>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasConstrains -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasConstrains">
        <rdfs:comment>building zone has constrains some acoustics requirements as defined by regulation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasJointInsulation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasJointInsulation">
        <rdfs:comment>A joint has joint insulation parameters</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasMeter -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasMeter">
        <rdfs:comment>A zone has some IoT equipment installed to measure acoustics related parameters</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundInsulation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundInsulation">
        <rdfs:comment>building element has sound insulation</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundMetrics -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundMetrics">
        <rdfs:comment>Relationship between the acoustic performance and the metrics considered from the zone</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundPerformance -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundPerformance">
        <rdfs:comment>A building zone has some sound performance</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundPowerLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundPowerLevel">
        <rdfs:comment>Sound source has some accoustics performance value</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundSource -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundSource">
        <rdfs:comment>A zone has a sound source</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSource -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSource">
        <rdfs:comment>Noise observation has source</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#observes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#observes">
        <rdfs:comment>sensor observation value</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasAcousticPerformance -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasAcousticPerformance">
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AverageNoiseLevel"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#PercentilNoiseLevel"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#ReverberationTime"/>
        <rdfs:comment>A list of acoustics peformance indicators</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription">
        <rdfs:comment>A description of the class</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName">
        <rdfs:comment>Class name</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasObservationType -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasObservationType">
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Noise"/>
        <rdfs:comment>Type of accoustics related observation</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasPublisher -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasPublisher">
        <rdfs:comment>A regulation publisher</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/bot#Element -->


    <owl:Class rdf:about="https://w3id.org/bot#Element">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/bot#Zone -->


    <owl:Class rdf:about="https://w3id.org/bot#Zone">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/bot#hasElement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/bot#Element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasConstrains"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticRequirements"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasMeter"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsMeter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundPerformance"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsPerformance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundSource"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#SoundSource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticParameters -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticParameters">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasPublisher"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A quality in the accoustic domain</rdfs:comment>
        <rdfs:label>AcousticQuality</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticRequirements -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticRequirements">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/Entities#InformationContentEntity"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/Information#RegulatoryOrder"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Requirements set by the regulation about acoustics parameters to be considered in the building environment or elements</rdfs:comment>
        <rdfs:label>AcousticsRegulation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsMeter -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsMeter">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The device in place to measure the level of noise</rdfs:comment>
        <rdfs:label>Acoustic Meter</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsObservation -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsObservation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSource"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#SoundSource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The list of acoustics measurements in the bulding environment as captured by the noise sensor</rdfs:comment>
        <rdfs:label>Acoustic Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsPerformance -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsPerformance">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/Information#PerformanceIndicator"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasConstrains"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticRequirements"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundMetrics"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsObservation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasAcousticPerformance"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Sensor based measurement of acoustics performance in the building environment</rdfs:comment>
        <rdfs:label>AcousticPerformance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsProperty -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticsProperty">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasObservationType"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The specific property to be measured by the sensing device</rdfs:comment>
        <rdfs:label>Acoustic Property</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundImprovement -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundImprovement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticParameters"/>
        <rdfs:comment>Airborne Sound Improvement for a building element</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundInsulation -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundInsulation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticParameters"/>
        <rdfs:comment>Airborne Sound Insulation  for a building element</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Door -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Door">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundInsulation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundInsulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Building Door</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Elevator -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Elevator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#ServiceEquipment"/>
        <rdfs:comment>Building Elevator</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#FacadeElement -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#FacadeElement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundInsulation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundInsulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Building Facade</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#FlankingSoundTransmission -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#FlankingSoundTransmission">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AcousticParameters"/>
        <rdfs:comment>Flanking  Sound Improvement for a building element</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Floor -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#Floor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundInsulation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundInsulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Building Floor</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#FloorCovering -->


    <owl:Class rdf:about="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#FloorCovering">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#hasSoundInsulation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/digitalconstruction/0.3/BuildingAcoustics#AirborneSoundInsulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
